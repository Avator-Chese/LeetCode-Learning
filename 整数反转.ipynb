    "def resverse_fun(x):\n",
    "    sign=\"\"\n",
    "    if x<0:\n",
    "        sign=\"-\"\n",
    "        x=-x\n",
    "    c=int(str(x)[::-1])\n",
    "    if c>2**31-1 or c<2**31*(-1):\n",
    "        return 0\n",
    "    else:\n",
    "        return sign+str(c)"
